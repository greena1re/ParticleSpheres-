class Node extends VerletParticle{

  Node(Vec3D pos) {
    super(pos);
  }

  // All we're doing really is adding a display() function to a VerletParticle
  void display() {
    fill(#0000AA);
    noStroke();
    lights; 
    specular; 
    //stroke(0);
   //ellipse(x,y, 12, 12);
    sphere(20);
  }
}